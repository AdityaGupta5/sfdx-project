<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Inspectors_Must_Work_Overtime</fullName>
    <active>true</active>
    <errorConditionFormula>AND( Name = &quot;Exhaust port inspector&quot;, Utilization__c  &lt; 1.5)</errorConditionFormula>
    <errorDisplayField>Utilization__c</errorDisplayField>
    <errorMessage>Exhaust Port Inspectors must work at least 150%. No coffee breaks!</errorMessage>
</ValidationRule>
